                                                @foreach (var item in miLista)
                                                {
                                                    <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                                                        <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                                            @item[0]
                                                        </a>
                                                        <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                                                            <li><a class="dropdown-item" href="@item[2]">@item[1]</a></li>
                                                        </ul>
                                                    </ul>
                                                } 
                                            }                            
                                        </li>
                                    </ul>
                                </div>
                            </div>
                        </nav>*@
                        <a class="nav-link text-dark" asp-controller="Login" asp-action="Cerrar">
                            <b style="color:#8B8B8B;">
                                Cerrar Sesión
                            </b>
                        </a>
                    </div>
                }
                else
                {

                    <div id="navcol-1" class="collapse navbar-collapse">
                        <a class="nav-link text-dark" asp-controller="Principal" asp-action="Index">
                            <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-home" width="36" height="36" viewBox="0 0 24 24" stroke-width="2" stroke="#1D4D87" fill="none" stroke-linecap="round" stroke-linejoin="round">
                                <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                                <path d="M5 12l-2 0l9 -9l9 9l-2 0" />
                                <path d="M5 12v7a2 2 0 0 0 2 2h10a2 2 0 0 0 2 -2v-7" />
                                <path d="M9 21v-6a2 2 0 0 1 2 -2h2a2 2 0 0 1 2 2v6" />
                            </svg>  
                        </a>
                        <ul class="navbar-nav mx-auto">
                        </ul>

                        <a class="nav-link text-dark" asp-area="" asp-controller="Login" asp-action="Login">
