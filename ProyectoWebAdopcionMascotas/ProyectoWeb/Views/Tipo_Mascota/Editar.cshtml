@model ProyectoWeb.Models.Tipo_Mascota

@{
    ViewData["Title"] = "Editar";
}

<h1 style="text-align:center">EDITAR TIPO MASCOTA</h1>

<h4>Tipo_Mascota</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Editar">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="idTipoMascota" class="control-label" ></label>
                <input asp-for="idTipoMascota" class="form-control" readonly/>
                <span asp-validation-for="idTipoMascota" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="tipoMascota" class="control-label">Tipo Mascota</label>
                <input asp-for="tipoMascota" class="form-control" />
                <span asp-validation-for="tipoMascota" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="estadoTipoMascota" class="control-label">Estado</label>
                <input asp-for="estadoTipoMascota" class="form-control" />
                <span asp-validation-for="estadoTipoMascota" class="text-danger"></span>
            </div>
            <br />
            <div class="form-group">
                <input type="submit" value="Guardar" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Mostrar">Regresar a la Lista</a>
</div>

